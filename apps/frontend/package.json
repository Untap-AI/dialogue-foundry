{
  "name": "@dialogue-foundry/frontend",
  "private": true,
  "version": "0.1.1",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "build:watch": "vite build --watch",
    "start": "vite",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "clean": "rm -rf dist && rm -rf node_modules",
    "typecheck": "tsc --noEmit",
    "publish-package": "node ../../packages/deploy-s3/dist/version-based-deploy.js --package ./package.json --source ./dist --bucket dialogue-foundry-frontend"
  },
  "dependencies": {
    "@dialogue-foundry/tailwind-config": "workspace:*",
    "@nlux/react": "^2",
    "@nlux/themes": "^2",
    "@tailwindcss/postcss": "^4.0.15",
    "@tailwindcss/vite": "^4.0.16",
    "autoprefixer": "^10.4.21",
    "axios": "^1.8.4",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "tailwindcss": "^4.0.15",
    "vite-plugin-css-injected-by-js": "^3.5.2"
  },
  "devDependencies": {
    "@babel/plugin-syntax-jsx": "^7.23.3",
    "@dialogue-foundry/deploy-s3": "workspace:*",
    "@dialogue-foundry/eslint-config": "workspace:*",
    "@dialogue-foundry/tsconfig": "workspace:*",
    "@types/node": "^20.17.24",
    "@types/react": "^19.0.10",
    "@types/react-dom": "^19.0.4",
    "@vitejs/plugin-react": "^4.3.4",
    "eslint": "^8.57.1",
    "typescript": "~5.3.3",
    "vite": "^6.2.0"
  }
}